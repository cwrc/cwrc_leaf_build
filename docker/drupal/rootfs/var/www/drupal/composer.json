  "require-dev": {
    "drupal/addtoany": "*",
    "drupal/better_exposed_filters": "*",
    "drupal/block_class": "*",
    "drupal/condition_query": "*",
    "drupal/conditional_fields": "*",
    "drupal/config_distro_filter": "*",
    "drupal/config_filter": "*",
    "drupal/config_profile": "*",
    "drupal/config_provider": "*",
    "drupal/config_split": "*",
    "drupal/config_update": "*",
    "drupal/context": "^5.0@RC",
    "drupal/core": "^10",
    "drupal/core-dev": "^10",
    "drupal/core-recommended": "^10",
    "drupal/devel": "*",
    "drupal/file_download_link": "*",
    "drupal/focal_point": "*",
    "drupal/form_options_attributes": "*",
    "drupal/formatter_suite": "*",
    "drupal/hal": "*",
    "drupal/jquery_ui_slider": "*",
    "drupal/jwt": "*",
    "drupal/layout_library": "*",
    "drupal/lb_claro": "*",
    "drupal/libraries": "*",
    "drupal/migrate_plus": "*",
    "drupal/migrate_tools": "*",
    "drupal/name": "*",
    "drupal/oembed_providers": "*",
    "drupal/registration_role": "*",
    "drupal/sendgrid_integration": "*",
    "drupal/simple_sitemap": "*",
    "drupal/social_link_field": "*",
    "drupal/token_filter": "*",
    "drupal/transliterate_filenames": "*",
    "drupal/twig_xdebug": "^1.2",
    "drupal/views_block_filter_block": "*",
    "drupal/views_flipped_table": "*",
    "drupal/views_show_more": "*",
    "mglaman/drupal-check": "^1.4",
    "mglaman/phpstan-drupal": "*"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "drupal/console-extend-plugin": true,
      "cweagans/composer-patches": true,
      "oomphinc/composer-installers-extender": true,
      "wikimedia/composer-merge-plugin": true,
      "zaporylie/composer-drupal-optimizations": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[web-root]/sites/default/default.settings.php": {
          "append": "assets/default.settings.php"
        }
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/profiles/{$name}": [
        "type:drupal-profile-custom"
      ]
    },
    "patches": {
      "drupal/group_content_menu": {
        "Add Drush commands": "patches/group_content_menu-menu-not-visible-3170013-17.patch",
        "When creating first menu link, it doesn't appear until the cache is cleared": "patches/group_content_menu-menu-link-not-showing-until-cache-clear-3329661-10.patch",
        "Operation is null in custom menu link form": "https://www.drupal.org/files/issues/2023-05-22/set-operations-3361822.patch"
      },
      "drupal/backup_migrate": {
        "Add Drush commands": "https://www.drupal.org/files/issues/2021-08-09/3223057-10.patch"
      },
      "islandora/controlled_access_terms": {
        "Normalize": "patches/controlled_access_terms.normalizer.patch"
      },
      "drupal/content_lock": {
        "Add Drush commands": "patches/content_lock_group_permission.patch",
        "Fix PHP 8.2+ deprecation notices": "https://www.drupal.org/files/issues/2023-05-22/3343964-5.patch"
      },
      "drupal/group": {
        "Attach Repository Item to group": "patches/repository_item_group_attach.patch",
        "Get group id from url on group content page": "https://www.drupal.org/files/issues/2020-02-10/views-cont-filter-to-get-gid-from-url-or-group-content-3112501.patch",
        "group_membership_duplication": "https://www.drupal.org/files/issues/2019-07-09/group_membership_duplication-3016197-10.patch",
        "Use of static in callables is deprecated ": "https://www.drupal.org/files/issues/2023-07-25/3349565-6.patch",
        "Revisions no longer work since Drupal 9.3": "https://www.drupal.org/files/issues/2024-02-29/revision-no-longer-works-3254038-23.patch",
        "Add revisions tab on groups": "https://www.drupal.org/files/issues/2023-08-24/groups-have-two-revisions-tabs.patch"
      },
      "drupal/bootstrap_barrio": {
        "Missing library fix": "patches/bootstrap-barrio-file-library-fix.patch"
      },
      "islandora/islandora_install_profile_demo": {
        "Repository item group content delete": "patches/repository_item_group_content_delete.patch"
      },
      "drupal/bootstrap_layout_builder": {
        "Fatal error fix": "https://www.drupal.org/files/issues/2023-03-09/bootstrap_layout_builder-attributes_classes-3343773-8.patch"
      },
      "islandora/title_field_for_manage_display": {
        "Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2023-11-08/title_field_for_manage_display-D10upgrade.patch"
      },
      "drupal/rest_oai_pmh": {
        "Array check issue fix": "patches/array_check_rest_oai_pmh.patch"
      },
      "drupal/core": {
        "Parent menu exist check D10.2": "patches/core/parent_menu_exist.patch"
      },
      "drupal/quickedit": {
        "Uncaught TypeError: _.where & _.pluck is not a function": "https://www.drupal.org/files/issues/2023-12-27/type-error-where-pluck-not-a-function-3410956-3.patch"
      },
      "drupal/require_revision_log_message": {
        "can this be made to work with media as well": "https://www.drupal.org/files/issues/2023-04-24/3293857-worksformedia-10.patch"
      }
    },
    "drupal-core-project-message": {
      "include-keys": [
        "homepage",
        "support"
      ],
      "post-create-project-cmd-message": [
        "<bg=blue;fg=white>                                                         </>",
        "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
        "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
        "<bg=blue;fg=white>                                                         </>",
        "",
        "<bg=yellow;fg=black>Next steps</>:",
        "  * Install the site: https://www.drupal.org/docs/8/install",
        "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
        "  * Get support: https://www.drupal.org/support",
        "  * Get involved with the Drupal community:",
        "      https://www.drupal.org/getting-involved",
        "  * Remove the plugin that prints this message:",
        "      composer remove drupal/core-project-message"
      ]
    }
  }
}
