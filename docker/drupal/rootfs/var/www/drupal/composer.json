    {
      "type": "package",
      "package": {
        "name": "library/openseadragon",
        "version": "2.4.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/openseadragon/openseadragon/releases/download/v2.4.2/openseadragon-bin-2.4.2.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "library/pdf.js",
        "version": "2.4.456",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/mozilla/pdf.js/releases/download/v2.4.456/pdfjs-2.4.456-dist.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "library/masonry",
        "version": "3.3.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/desandro/masonry/archive/v3.3.2.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "imagesloaded/imagesloaded",
        "version": "v4.1.4",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/desandro/imagesloaded/archive/v4.1.4.zip",
          "type": "zip"
        },
        "extra": {
          "installer-name": "imagesloaded"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    {
      "type": "package",
      "package": [
        {
          "name": "cwrc/leafwriter",
          "version": "3.5.0",
          "type": "drupal-library",
          "extra": {
            "installer-name": "leafwriter"
          },
          "dist": {
            "url": "https://registry.npmjs.org/@cwrc/leafwriter/-/leafwriter-3.5.0.tgz",
            "type": "tar"
          }
        }
      ]
    },
    {
      "type": "package",
      "package": [
        {
          "name": "cwrc/dtoc",
          "version": "0.3.10",
          "type": "drupal-library",
          "extra": {
            "installer-name": "leafdtoc"
          },
          "dist": {
            "url": "https://registry.npmjs.org/@cwrc/dtoc/-/dtoc-0.3.10.tgz",
            "type": "tar"
          }
        }
      ]
    }
  ],
  "authors": [
    {
      "name": "dannylamb",
      "email": "dlamb@islandora.ca"
    }
  ],
