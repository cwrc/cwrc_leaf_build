{
  "name": "islandora/leaf",
  "type": "project",
  "description": "LEAF customizations for Islandora.",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Islandora-Devops/islandora_base_theme.git",
      "no-api": true
