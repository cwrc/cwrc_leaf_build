    "drupal/views_local_tasks": "^1.0",
    "drush/drush": "^12",
    "islandora-rdm/islandora_fits": "dev-8.x-1.x",
    "islandora/controlled_access_terms": "*",
    "islandora/islandora": "^2.10",
    "islandora/islandora_mirador": "*",
    "islandora/openseadragon": "2.x-dev",
    "j7mbo/twitter-api-php": "^1.0",
    "library/pdf.js": "2.4.456",
    "mjordan/islandora_workbench_integration": "dev-main",
    "oomphinc/composer-installers-extender": "^2",
    "vlucas/phpdotenv": "4.2.0",
    "webflo/drupal-finder": "^1.2",
    "wikimedia/composer-merge-plugin": "^2.0",
    "zaporylie/composer-drupal-optimizations": "^1.2"
